{{> header}}

<h2 class="section-title">Главные новости</h2>

<div class="all-main">
  {{#each all1}}
    <div class="all-item all-item_{{@index}}">
      <a href="/articles/{{this.slug}}">
        <img class="all-item_img" src="{{this.img}}">
        <span class="all-item_link" href="/articles/{{this.slug}}">{{this.title}}</span>
      </a>
      <span class="all-item_category">{{this.category}}</span>
    </div>
  {{/each}}
</div>

<h2 class="section-title">Последние новости</h2>

<div class="three-columns">
  {{#each all2}}
    <div class="all-item all-item_{{@index}}">
      <img class="all-item_img" src="{{this.img}}">
      <a class="all-item_link" href="/articles/{{this.slug}}">{{this.title}}</a>
      <span class="all-item_category">{{this.category}}</span>
    </div>
  {{/each}}
</div>

<h2 class="section-title">Политика</h2>

<section class="category-posts three-columns">
  {{#each politic}}
